        $("#gst_amount").val(g_amount);
        $("#grand_amount").val(t_amount);
        calculateTotal();
        // console.log(addOnData[key].addOn_price);
    }

    function Decreament(value, key) {
        var itemQty = value.value;
        if (itemQty > 1) {
            itemQty--;

            value.value = itemQty;
            addOnData[key].addOn_qty = itemQty;
            addOnData[key].addOn_price = parseFloat(parseFloat(addOnData[key].addOn_price) - parseFloat(addOnData[key]
                .addOn_unit_price));
            t_amount = parseFloat(t_amount - parseFloat(addOnData[key].addOn_unit_price));
            $("#total_amount").val(t_amount);
            $("#gst_amount").val(g_amount);
            $("#grand_amount").val(t_amount);
            // alert(t_amount);
        }
        calculateTotal();
    }

    function onlyNumberKey(evt) {

        // Only ASCII character in that range allowed
        var ASCIICode = (evt.which) ? evt.which : evt.keyCode
        if (ASCIICode > 31 && (ASCIICode < 48 || ASCIICode > 57))
            return false;
        return true;
    }

