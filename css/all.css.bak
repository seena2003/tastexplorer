    $(document).ready(function() {

        $(document).on('click', '.location-selector', function() {
            // Get all child elements with the class "child"
            const childElements = document.querySelectorAll('.location-image');

            // Attach click event listeners to each child element
            childElements.forEach(child => {
                child.addEventListener('click', (event) => {
